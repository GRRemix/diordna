<?xml version="1.0" encoding="UTF-8"?>
<manifest>
	<remote name="github"
	   		fetch="https://github.com"
	   		revision="nougat"
	   		sync-c="true"
			sync-f="true"
           	sync-j="8" />

	<remote name="aosp"
			fetch="https://android.googlesource.com"
			review="android-review.googlesource.com" 
			revision="refs/tags/android-7.1.2_r36"
			sync-c="true"
			sync-f="true"
			sync-j="8" 
		clone-depth="1" />

	<default remote="aosp" />

	<include name="snippets/aosp.xml" />
	<include name="snippets/gemini.xml" />
	<include name="snippets/rremix.xml" />
	<include name="snippets/los.xml" />
</manifest>
